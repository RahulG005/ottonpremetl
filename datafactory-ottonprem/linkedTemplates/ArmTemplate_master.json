{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-ottonprem"
		},
		"filesystem_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'filesystem_ls'"
		},
		"ls_adls_api_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_api'"
		},
		"ls_movie_Recommendation_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_movie_Recommendation'"
		},
		"filesystem_ls_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\Rahul\\Raw data storage\\ottonpremdata\\Host"
		},
		"filesystem_ls_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "RAHUL"
		},
		"kv_ls_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://ottkeyvault011.vault.azure.net/"
		},
		"ls_adls2_ingest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageottonprem011.dfs.core.windows.net"
		},
		"ls_adls_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageottonprem011.dfs.core.windows.net/"
		},
		"ls_api_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://my.api.mockaroo.com/users.json?"
		},
		"ls_azure_sql_database_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ottsqlserver011.database.windows.net"
		},
		"ls_azure_sql_database_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "ottsqldatabase011"
		},
		"ls_azure_sql_database_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Rahul"
		},
		"ls_movie_Recommendation_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageottonprem011.dfs.core.windows.net/"
		},
		"ls_transform_notebook_properties_typeProperties_endpoint": {
			"type": "string",
			"defaultValue": "https://synapseott.dev.azuresynapse.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-ottonprem_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_api_accountKey": {
						"value": "[parameters('ls_adls_api_accountKey')]"
					},
					"ls_movie_Recommendation_accountKey": {
						"value": "[parameters('ls_movie_Recommendation_accountKey')]"
					},
					"filesystem_ls_password": {
						"value": "[parameters('filesystem_ls_password')]"
					},
					"kv_ls_properties_typeProperties_baseUrl": {
						"value": "[parameters('kv_ls_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_api_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_api_properties_typeProperties_url')]"
					},
					"ls_api_properties_typeProperties_url": {
						"value": "[parameters('ls_api_properties_typeProperties_url')]"
					},
					"ls_movie_Recommendation_properties_typeProperties_url": {
						"value": "[parameters('ls_movie_Recommendation_properties_typeProperties_url')]"
					},
					"ls_transform_notebook_properties_typeProperties_endpoint": {
						"value": "[parameters('ls_transform_notebook_properties_typeProperties_endpoint')]"
					},
					"filesystem_ls_properties_typeProperties_host": {
						"value": "[parameters('filesystem_ls_properties_typeProperties_host')]"
					},
					"filesystem_ls_properties_typeProperties_userId": {
						"value": "[parameters('filesystem_ls_properties_typeProperties_userId')]"
					},
					"ls_adls2_ingest_properties_typeProperties_url": {
						"value": "[parameters('ls_adls2_ingest_properties_typeProperties_url')]"
					},
					"ls_azure_sql_database_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_database_properties_typeProperties_server')]"
					},
					"ls_azure_sql_database_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_database_properties_typeProperties_database')]"
					},
					"ls_azure_sql_database_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_database_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "datafactory-ottonprem_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'datafactory-ottonprem_ArmTemplate_0')]"
			]
		}
	]
}